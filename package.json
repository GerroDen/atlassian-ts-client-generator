{
  "name": "atlassian-js-client-generator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "generated-code/*"
  ],
  "scripts": {
    "fetch": "run-s 'fetch:*'",
    "fetch:bamboo-api": "curl -o bamboo-api/swagger.json https://dac-static.atlassian.com/server/bamboo/swagger.v3.json",
    "fetch:bitbucket-api": "curl -o bitbucket-api/swagger.json https://dac-static.atlassian.com/server/bitbucket/9.2.swagger.v3.json",
    "fetch:confluence-cloud-api-v1": "curl -o confluence-cloud-api-v1/swagger.json https://dac-static.atlassian.com/cloud/confluence/swagger.v3.json",
    "fetch:confluence-cloud-api-v2": "curl -o confluence-cloud-api-v2/swagger.json https://dac-static.atlassian.com/cloud/confluence/openapi-v2.v3.json",
    "fetch:jira-cloud-api": "curl -o jira-cloud-api/swagger.json https://dac-static.atlassian.com/cloud/jira/platform/swagger-v3.v3.json",
    "fetch:jira-cloud-service-management-api": "curl -o jira-cloud-service-management-api/swagger.json https://dac-static.atlassian.com/cloud/jira/service-desk/swagger.v3.json?_v=1.7471.0-0.1310.0",
    "fetch:jira-software-cloud-api": "curl -o jira-software-cloud-api/swagger.json https://dac-static.atlassian.com/cloud/jira/software/swagger.v3.json",
    "fetch:migration-api": "curl -o migration-api/swagger.json https://dac-static.atlassian.com/platform/app-migration/swagger.v3.json",
    "fetch:teams-cloud-api": "curl -o teams-cloud-api/swagger.json https://dac-static.atlassian.com/platform/teams/swagger-public-api-v1.v3.json",
    "fetch:user-management-cloud-api": "curl -o user-management-cloud-api/swagger.json https://dac-static.atlassian.com/cloud/admin/user-management/swagger.v3.json",
    "gen": "run-s 'gen:*'",
    "gen:bamboo-api": "run-s 'gen:bamboo-api:*'",
    "gen:bamboo-api:gen": "gen-tool/gen-api.js bamboo-api",
    "gen:bamboo-api:lint": "tsc -p generated-code/bamboo-api --noEmit",
    "gen:bitbucket-api": "run-s 'gen:bitbucket-api:*'",
    "gen:bitbucket-api:gen": "gen-tool/gen-api.js bitbucket-api",
    "gen:bitbucket-api:fix": "jscodeshift -t gen-tool/codemods/fix-bitbucket-api.cjs --parser=ts --extensions=ts generated-code/bitbucket-api/src",
    "gen:bitbucket-api:lint": "tsc -p generated-code/bitbucket-api --noEmit",
    "gen:confluence-cloud-api-v1": "run-s 'gen:confluence-cloud-api-v1:*'",
    "gen:confluence-cloud-api-v1:gen": "gen-tool/gen-api.js confluence-cloud-api-v1",
    "gen:confluence-cloud-api-v1:fix": "jscodeshift -t gen-tool/codemods/fix-confluence-cloud-api-v1.cjs --parser=ts --extensions=ts generated-code/confluence-cloud-api-v1/src",
    "gen:confluence-cloud-api-v1:lint": "tsc -p generated-code/confluence-cloud-api-v1 --noEmit",
    "gen:confluence-cloud-api-v2": "run-s 'gen:confluence-cloud-api-v2:*'",
    "gen:confluence-cloud-api-v2:gen": "gen-tool/gen-api.js confluence-cloud-api-v2",
    "gen:confluence-cloud-api-v2:fix": "jscodeshift -t gen-tool/codemods/fix-confluence-cloud-api-v2.cjs --parser=ts --extensions=ts generated-code/confluence-cloud-api-v2/src",
    "gen:confluence-cloud-api-v2:lint": "tsc -p generated-code/confluence-cloud-api-v2 --noEmit",
    "gen:jira-cloud-api": "run-s 'gen:jira-cloud-api:*'",
    "gen:jira-cloud-api:gen": "gen-tool/gen-api.js jira-cloud-api",
    "gen:jira-cloud-api:pre-fix": "gen-tool/fix-jira-cloud-gen.js generated-code/jira-cloud-api",
    "gen:jira-cloud-api:fix": "jscodeshift -t gen-tool/codemods/fix-jira-cloud-api.cjs --parser=ts --extensions=ts generated-code/jira-cloud-api/src",
    "gen:jira-cloud-api:lint": "tsc -p generated-code/jira-cloud-api --noEmit",
    "gen:jira-cloud-service-management-api": "run-s 'gen:jira-cloud-service-management-api:*'",
    "gen:jira-cloud-service-management-api:gen": "gen-tool/gen-api.js jira-cloud-service-management-api",
    "gen:jira-cloud-service-management-api:fix": "jscodeshift -t gen-tool/codemods/fix-jira-cloud-service-management-api.cjs --parser=ts --extensions=ts generated-code/jira-cloud-service-management-api/src",
    "gen:jira-cloud-service-management-api:lint": "tsc -p generated-code/jira-cloud-service-management-api --noEmit",
    "gen:jira-software-cloud-api": "run-s 'gen:jira-software-cloud-api:*'",
    "gen:jira-software-cloud-api:gen": "gen-tool/gen-api.js jira-software-cloud-api",
    "gen:jira-software-cloud-api:fix": "jscodeshift -t gen-tool/codemods/fix-jira-software-cloud-api.cjs --parser=ts --extensions=ts generated-code/jira-software-cloud-api/src",
    "gen:jira-software-cloud-api:lint": "tsc -p generated-code/jira-software-cloud-api --noEmit",
    "gen:migration-api": "run-s 'gen:migration-api:*'",
    "gen:migration-api:gen": "gen-tool/gen-api.js migration-api",
    "gen:migration-api:lint": "tsc -p generated-code/migration-api --noEmit",
    "gen:teams-cloud-api": "run-s 'gen:teams-cloud-api:*'",
    "gen:teams-cloud-api:gen": "gen-tool/gen-api.js teams-cloud-api",
    "gen:teams-cloud-api:lint": "tsc -p generated-code/teams-cloud-api --noEmit",
    "gen:user-management-cloud-api": "run-s 'gen:user-management-cloud-api:*'",
    "gen:user-management-cloud-api:gen": "gen-tool/gen-api.js user-management-cloud-api",
    "gen:user-management-cloud-api:lint": "tsc -p generated-code/user-management-cloud-api --noEmit",
    "lint": "run-p 'lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier '{**/*,*}.{js,cjs,json}' --check",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "openapi-generator-version": "openapi-generator-cli version-manager list"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@openapitools/openapi-generator-cli": "^2.13.12",
    "@types/jest": "^29.5.13",
    "@types/jscodeshift": "^0.12.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "exec-sh": "^0.4.0",
    "fast-glob": "^3.3.2",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "jscodeshift": "^17.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  }
}
